<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <!-- General -->
    <sys:String x:Key="Ok">OK</sys:String>
    <sys:String x:Key="Cancel">Cancel</sys:String>
    <sys:String x:Key="Close">Close</sys:String>
    <sys:String x:Key="NotImplementedTitle">Not implemented</sys:String>
    <sys:String x:Key="NotImplementedMessage">This feature is not implemented yet.</sys:String>
    
</ResourceDictionary>